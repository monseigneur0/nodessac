
<form action="../rec" method="post">

    <label for="name">이름</label>
    <input type="text" class="form-control" name="name" id="name" placeholder="홍길동" value="" >
    <label for="nickname">아이디</label>
    <input type="text" class="form-control" name="id" id="id" placeholder="hong123" value="" >
    <label for="password">비밀번호</label>
    <input type="password" class="form-control" name="pw" id="" placeholder="abc123!" required>
    <label for="email">이메일</label>
    <input type="text" class="form-control" id="email" placeholder="you@example.com" required>
    <label for="genderid">성별</label>
    <select class="form-select d-block w-100" id="gender"  name="gender">
        <option value="male" selected>남자</option>
        <option value="female">여자</option>
    </select>
    <label for="root"> 생년월일</label>
    <input type="date" name="birthday" value="2021-10-21" min="1979-09-22" max="2021-10-22">
    <button type="submit">제출</button>
</form>